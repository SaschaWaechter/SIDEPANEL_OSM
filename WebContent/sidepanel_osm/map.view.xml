<core:View xmlns:core="sap.ui.core" xmlns:mvc="sap.ui.core.mvc" xmlns="sap.m" xmlns:vbm="sap.ui.vbm"
		controllerName="sidepanel_osm.map" xmlns:html="http://www.w3.org/1999/xhtml">
	<Page id="mainPage" enableScrolling="false" showHeader="false">
                <vbm:GeoMap id="GeoMap" width="100%" height="100%">   
                <vbm:vos>
                        <vbm:Spots items="{/Spots}">
                            <vbm:Spot position="{pos}" tooltip="{tooltip}" type="Default" text="" />
                        </vbm:Spots>
                </vbm:vos>   
                </vbm:GeoMap>
	</Page>
</core:View>